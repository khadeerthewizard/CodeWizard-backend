def print_diamond(n):
    if n % 2 == 0:
        print("Please enter an odd number.\n")
        return

    # Upper half of the diamond
    for i in range(1, n + 1, 2):
        print(" " * ((n - i) // 2) + "*" * i)

    # Lower half of the diamond
    for i in range(n - 2, 0, -2):
        print(" " * ((n - i) // 2) + "*" * i)

if __name__ == "__main__":
    n = int(input("Enter the number of rows (odd number): \n"))
    print_diamond(n)
